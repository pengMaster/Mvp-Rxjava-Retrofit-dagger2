<?xml version="1.0" encoding="utf-8"?><!-- 圆环 -->
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 填充的颜色 -->
    <solid android:color="@color/white" />
    <!-- 线的宽度，颜色灰色 -->
    <stroke
        android:width="1dp"
        android:color="@color/gray" />
    <!-- android:radius 弧形的半径 -->
    <corners android:radius="5dp" />
</shape>